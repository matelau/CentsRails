	<script>
    if (sessionStorage.getItem("colors"))
    {
    	var c = jQuery.parseJSON(unescape(sessionStorage.getItem("colors")));
        $('.btn-default').css({"color":c["p_hex"]});
        $('.main-search').css({"border-color":c["p_hex"]});
        $('#income-text').css({"color":c["p_hex"]});
        $('.btn-primary').css({"background-color":c["p_hex"]});
        $('#default-button').css("cssText", "background-color: rgb(" + (c["p_rgb"][0]-5) + "," + (c["p_rgb"][1]-5) + "," + (c["p_rgb"][2]-5) + ") !important");
    }
    else
    {
        $('#default-button').css("cssText", "background-color: rgb(" + (138-5) + "," + (68-5) + "," + (18-5) + ") !important");
    }
	</script>
