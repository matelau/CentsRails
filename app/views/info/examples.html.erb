    	<p id="error"></p>
	</div>
	<script>
		var qError = localStorage.getItem("stored_query");
		if(qError != null){
			document.getElementById("erModal").style.display = "block";
			document.getElementById("error").innerHTML = "Your question '" + qError + "' hasn't returned any data. Here are some categories you can use to search for something more specific."
			localStorage.removeItem("stored_query");
		}
