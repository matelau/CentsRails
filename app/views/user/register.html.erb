<%= form_tag("/register", method: "get") do %>
	<table>
		<tr>
			<td><%= label_tag(:first_name, "First name") %></td>
			<td><%= text_field_tag(:first_name) %></td>
		</tr>

		<tr>
			<td><%= label_tag(:last_name, "Last name") %></td>
			<td><%= text_field_tag(:last_name) %></td>
		</tr>

		<tr>
			<td><%= label_tag(:email_address, "Email") %></td>
			<td><%= text_field_tag(:email_address) %></td>
		</tr>

		<tr>
			<td><%= label_tag(:password, "Password") %></td>
			<td><%= password_field_tag(:password) %></td>
		</tr>

		<tr>
			<td><%= label_tag(:password_confirmation, "Confirm password") %></td>
			<td><%= password_field_tag(:password_confirmation) %></td>
		</tr>

		<tr>
			<td><%= submit_tag("Register") %></td>
		</tr>
	</table>
<% end %>